{
    "name": "hanafalah/microtenant",
    "description": "Package ini memungkinkan anda agar dapat mengelola multi tenant beserta dengan versioning didalamnya",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Hanafalah\\MicroTenant\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "authors": [
        {
            "name": "hamzah",
            "email": "hamzahnafalah@gmail.com"
        }
    ],
    "require": {
        "stancl/tenancy": "^3.8",
        "hanafalah/laravel-support": "1.x-dev as 1.0",
        "hanafalah/api-helper": "^1.0.0",
        "hanafalah/laravel-feature": "1.x-dev as 1.0",
        "hanafalah/module-user": "1.x-dev as 1.0",
        "hanafalah/module-workspace": "1.x-dev as 1.0",
        "hanafalah/laravel-permission": "1.x-dev as 1.0",
        "symfony/filesystem": "^7.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hanafalah\\MicroTenant\\MicroTenantServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}